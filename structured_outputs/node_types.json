{
    "type": "json_schema",
    "json_schema": {
        "name": "node_types",
        "schema": {
            "type": "object",
            "properties": {
                "matched_node_types": {
                    "type": "array",
                    "items": {
                        "type": "object",
                        "properties": {
                            "input_object_id": {
                                "type": "string",
                                "description": "The id of the node input object that describes the node."
                            },
                            "node_id": {
                                "type": "string",
                                "description": "The node id found in the node documentation."
                            }
                        },
                        "required": ["input_object_id", "node_id"]
                    }
                }
            },
            "required": ["matched_node_types"],
            "strict": true
        }
    }
}
